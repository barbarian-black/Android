<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.whack"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Game_activity"
            android:label="@string/title_activity_game_activity" >
        </activity>
        <activity
            android:name=".Game_Exit_Activity"
            android:label="@string/title_activity_game__exit_" >
        </activity>
        <activity
            android:name=".Capture_face"
            android:label="@string/title_activity_capture_face" >
        </activity>
    </application>

</manifest>
